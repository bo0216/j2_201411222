
package com.sd.turtle;

import java.awt.Color;
import ch.aplu.turtle.*;

public class Weightturtle extends Turtle {
    protected int weight;
    public Weightturtle(int w) {
        weight=w;
        setColor(Color.red);
        setPenColor(Color.red);
    }
    public int getWeight() {
        return weight;
    }
}